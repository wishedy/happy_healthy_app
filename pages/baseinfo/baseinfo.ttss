@import "../../styles/common.ttss";.xyh-form-title{padding-top:10px;display:block;padding-left:18px;margin-bottom:8px;height:20px;font-size:14px;font-family:PingFang-SC-Medium,PingFang-SC;font-weight:500;color:#4d4d4d;line-height:20px}.xyh-form-box{width:339px;height:48px;background:white;border-radius:6px;display:flex;flex-direction:row;align-items:center;justify-content:center;margin:0 auto}.xyh-form-box .wrap{padding-left:10px;width:315px;height:30px;background:whitesmoke;border-radius:6px;position:relative}.xyh-form-box .wrap .arrow{width:20px;height:20px;background:#0af;position:absolute;right:6px;top:50%;transform:translateY(-50%)}.xyh-form-box .wrap input,.xyh-form-box .wrap .picker{width:100%;height:100%;outline:0;border:0;font-size:12px;font-family:PingFangSC-Regular,PingFang SC;font-weight:400;color:#4d4d4d;line-height:30px}.xyh-form-box .wrap .date{font-size:12px;color:#b3b3b3;line-height:60rpx}.xyh-form-panel{width:329px;height:20px;background:white;border-radius:6px;margin:0 auto;padding:20px 0 16px 10px;display:flex;flex-direction:row;justify-content:flex-start;flex-wrap:wrap}.xyh-form-panel .xyh-form-radio{width:110px;height:20px;display:flex;flex-direction:row;align-items:center;justify-content:flex-start;margin-bottom:14px}.xyh-form-panel .xyh-form-radio.active .icon{border:1px solid #40a9ff;background:url("http://qn.xueyanhui.com/FnfkHkpOlylsK3EqGroy9als6-os") no-repeat center center/cover}.xyh-form-panel .xyh-form-radio:nth-of-type(3n){width:78px}.xyh-form-panel .xyh-form-radio .icon{width:12px;height:12px;border:1px solid gainsboro;margin-right:3px;border-radius:50%}.xyh-form-panel .xyh-form-radio .type{font-size:14px;font-family:PingFangSC-Regular,PingFang SC;font-weight:400;color:#4d4d4d}.jd_order_submit{line-height:100rpx;text-align:center;color:#fff;background:#55c47d;height:100rpx;box-shadow:-1rpx 8rpx 1rpx 0rpx rgba(161,161,161,0.06);display:flex;flex-direction:row;align-items:center;justify-content:center;position:fixed;bottom:0;left:0;right:0}
